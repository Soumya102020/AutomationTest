{
  "name": "admin-ui-tests",
  "version": "1.0.0",
  "description": "Acceptance and system tests for admin-ui",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "system-test": "mocha --require @babel/register --recursive --config=config.json system -R mocha-spec-json-output-reporter --reporter-options fileName=mocha-output.json,hierarchy=true",
    "system-test:debug": "mocha --debug --require @babel/register --recursive --config=config.json system",
    "system-test:watch": "mocha --require @babel/register --recursive -R dot --watch --config=config.json system",
    "system-test:quiet": "mocha --require @babel/register --recursive --config=config.json system; true",
    "acceptance-test": "wdio wdio.browserstack.conf.js",
    "local": "wdio wdio.conf.js",
    "eslint": "eslint .",
    "acceptance-test-debug": "wdio wdio.debug.conf.js",
    "config": "node swimlane-config.js",
    "send-coverage-data": "node ./acceptance/send-to-metrics.js"
  },
 
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-pipeline-operator": "^7.8.3",
    "@babel/plugin-proposal-throw-expressions": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@wdio/browserstack-service": "^5.18.1",
    "@wdio/cli": "^5.18.6",
    "@wdio/local-runner": "^5.18.6",
    "@wdio/mocha-framework": "^5.18.6",
    "@wdio/reporter": "^5.18.6",
    "@wdio/selenium-standalone-service": "^5.16.10",
    "@wdio/spec-reporter": "^5.18.6",
    "@wdio/sync": "^5.18.6",
    "asyncawait": "^3.0.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "bluebird": "3.7.2",
    "chai": "4.2.0",
    "chromedriver": "^79.0.0",
    "eslint": "6.8.0",
    "eslint-config-strict": "14.0.1",
    "eslint-plugin-filenames": "1.3.2",
    "lodash": "^4.17.15",
    "mailtrap": "^2.0.2",
    "mocha": "7.0.0",
    "mocha-spec-json-output-reporter": "1.1.7",
    "moment": "2.24.0",
    "nconf": "0.10.0",
    "node-uuid": "1.4.8",
    "supertest": "4.0.2",
    "wdio-chromedriver-service": "^5.0.2",
    "wdio-iedriver-service": "^0.1.0",
    "wdio-json-reporter": "^1.3.1"
  },
  "dependencies": {
    "github-generate-token": "^3.0.0",
    "webdriverio": "^5.18.6"
  }
}
